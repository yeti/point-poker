{
  "name": "point-poker",
  "version": "1.1.0",
  "description": "Point poker using sockets",
  "engines": {
    "node": ">=4.0.0"
  },
  "watch": {
    "server": [
      "lib",
      "index.js"
    ],
    "build:js": {
      "patterns": [
        "src"
      ],
      "extensions": "js",
      "quiet": false
    },
    "build:sass": {
      "patterns": [
        "src"
      ],
      "extensions": "scss",
      "quiet": false
    }
  },
  "scripts": {
    "start": "node index.js",
    "preserver": "npm run lint:server",
    "server": "node index.js & gulp reload",
    "dev": "npm-watch",
    "build": "npm run build:js && npm run build:sass",
    "prebuild:js": "npm run lint:client",
    "build:js": "rollup --config rollup.config.js",
    "build:sass": "gulp sass",
    "lint:server": "eslint index.js lib/**/*.js -c ./.eslintrc-server.js",
    "lint:client": "eslint src/**/*.js -c ./.eslintrc-client.js",
    "test": "mocha"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "chai": "^3.5.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "inherits": "^2.0.3",
    "mocha": "^3.3.0",
    "npm-watch": "^0.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "rollup": "0.41.6",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-commonjs": "8.1.0",
    "rollup-plugin-eslint": "3.0.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "1.0.1",
    "rollup-watch": "3.2.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "eslint-plugin-node": "^4.2.2",
    "express": "^4.15.2",
    "express-force-https": "^1.0.0",
    "lodash": "^4.17.4",
    "prettyjson": "^1.2.1",
    "react-addons-css-transition-group": "^15.5.2",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^15.4.2",
    "react-dropdown": "^1.2.3",
    "react-router": "^3.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-transition": "0.0.6",
    "react-tooltip": "^3.3.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "starwars-names": "^1.6.0"
  }
}
